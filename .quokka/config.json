{
  "pro": true,
  "installPackageCommand": "pnpm add {packageName}",
  "delay": 1500,
  "showValueOnSelection": true,
  "plugins": [
    "quokka-plugin-subdir",
    "jsdom-quokka-plugin",
    "alias-quokka-plugin",
    "./plugins/benchmark"
  ],
  "env": {
    "params": {
      "runner": "--experimental-specifier-resolution=node"
    }
  }
}
