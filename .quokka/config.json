{
  "pro": true,
  "node": "~/.quokka/quokka-node",
  "stdEsm": false,
  "nativeEsm": true,
  "installPackageCommand": "pnpm add {packageName}",
  "delay": 1500,
  "showValueOnSelection": true,
  "plugins": [
    "quokka-plugin-subdir",
    "jsdom-quokka-plugin",
    "alias-quokka-plugin",
    "./plugins/benchmark"
  ],
  "ts": {
    "compilerOptions": {
      "types": ["node"]
    }
  },
  "env": {
    "params": {
      "runner": "--experimental-specifier-resolution=node"
    }
  }
}
